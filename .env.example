# Living Heirloom Environment Configuration

# Application Settings
VITE_APP_NAME="Living Heirloom"
VITE_APP_VERSION="1.0.0"

# Feature Flags
VITE_ENABLE_AI=true
VITE_ENABLE_VOICE_FEATURES=true
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_ENCRYPTION=true

# AI Configuration
VITE_AI_MODEL_NAME="Llama-3.2-3B-Instruct-q4f32_1-MLC"
VITE_AI_TIMEOUT_MS=120000
VITE_AI_MAX_TOKENS=800
VITE_AI_TEMPERATURE=0.7

# Voice Configuration (ElevenLabs)
VITE_ELEVENLABS_API_KEY=""
VITE_VOICE_SAMPLE_RATE=44100
VITE_VOICE_MAX_DURATION=120
VITE_VOICE_MIN_DURATION=3

# Storage Configuration
VITE_DB_NAME="LivingHeirloomDB"
VITE_MAX_STORAGE_SIZE=104857600
VITE_DATA_RETENTION_DAYS=365

# Performance Settings
VITE_CHUNK_SIZE=1048576
VITE_MAX_CONCURRENT_REQUESTS=3
VITE_CACHE_TIMEOUT=300000

# Security Settings
VITE_ENCRYPTION_ALGORITHM="AES-GCM"
VITE_KEY_LENGTH=256
VITE_SALT_LENGTH=16

# Development Settings
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL="info"